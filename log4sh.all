logger_trace() {
    echo "Entering: $1" >&2
    trap "ret=\$?; started=\"$(date +%s%3N)\"; finished=\$(date +%s%3N); echo \"Finished (\$ret): $1 in \$((\$finished - \$started))ms\" >&2" EXIT;
}
logger_debug() { echo "$1" >&2; }
logger_info() { echo "$1" >&2; }
logger_warn() { echo "$1" >&2; }
logger_error() { echo "$1" >&2; }
logger_fatal() { echo "$1" >&2; }
