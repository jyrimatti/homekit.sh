#! /usr/bin/env nix-shell
#! nix-shell --keep LOGGING_LEVEL --keep PROFILING -i dash -I channel:nixos-23.05-small -p dash nodejs
. ./logging
. ./profiling
set -eu

logger_info 'Invoked api/identify'
logger_trace 'api/identify'

for p in store/pairings/*; do
    if test -d "$p"; then
        ./util/respond.sh 400 '{ "status": -70401 }'
        exit 1
    fi
done

./util/respond.sh 204
