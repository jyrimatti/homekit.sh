
logger_trace() { return 0; }
logger_debug() { return 0; }
logger_info()  { return 0; }
logger_warn()  { return 0; }
logger_error() { logger -i -t homekit.sh -p user.err  "[${REMOTE_ADDR:-}:${REMOTE_PORT:-}] $(basename "$0") - $1"; }
logger_fatal() { logger -i -t homekit.sh -p user.crit "[${REMOTE_ADDR:-}:${REMOTE_PORT:-}] $(basename "$0") - $1"; }
